{
    "name": "chrmina/cakespreadsheet",
    "type": "cakephp-plugin",
    "description": "CakePHP excel spreadsheet generator",
    "keywords": [
        "cakephp",
        "chrmina",
        "excel",
        "phpspreadsheet"
    ],
    "homepage": "https://github.com/chrmina/CakeSpreadsheet",
    "license": "MIT",
    "authors": [
        {
            "name": "Christakis Mina",
            "email": "christosmina@gmail.com"
        }
    ],
    "modifier": [
        {
            "name": "Christakis Mina",
            "email": "christosmina@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "cakephp/cakephp": "^3.6",
        "phpoffice/phpspreadsheet": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "CakeSpreadsheet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeSpreadsheet\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "support": {
        "issues": "https://github.com/chrmina/CakeSpreadsheetissues",
        "source": "https://github.com/chrmina/CakeSpreadsheet"
    }
}
